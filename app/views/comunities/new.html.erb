<%= form_for @community do |f| %>
  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :password %>:
  <%= f.text_field :password %><br />
  
  <%= f.label :password_confirmation %>:
  <%= f.text_field :password_confirmation %><br />
  
  <%= f.label :name %>:
  <%= f.text_field :name %><br />
  
  <%= f.label :description %>:
  <%= f.text_field :description %><br />
  
  <%= f.label :number %>:
  <%= f.text_field :number %><br />
  
  <%= f.label :website %>:
  <%= f.text_field :website %><br />
  
  <%= f.label :total_loan_amount %>:
  <%= f.text_field :total_loan_amount %><br />
  
  <%= f.label :max_loan_amount %>:
  <%= f.text_field :max_loan_amount %><br />
  
  <%= f.label :interest_rate %>:
  <%= f.text_field :interest_rate %><br />
  
  <%= f.label :loan_period %>:
  <%= f.text_field :loan_period %><br />
  
  <%= f.label :terms %>:
  <%= f.text_field :terms %><br />

  <%= f.submit %>
<% end %>